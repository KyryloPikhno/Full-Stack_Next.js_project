{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/**"]
      }
    }
  ],
  "build": {
    "env": {
      "DATABASE_URL": "@database_url"
    },
    "cmd": "yarn build && yarn prisma:generate"
  }
}
